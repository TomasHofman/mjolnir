<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>
    </ui:style>
    <g:HTMLPanel>
        <table>
            <tr>
                <td>Kerberos Name: </td>
                <td><g:TextBox ui:field="kerberosNameBox"/></td>
            </tr>
            <tr>
                <td>GitHub Name: </td>
                <td><g:TextBox ui:field="gitHubNameBox"/></td>
            </tr>
            <tr>
                <td>Note: </td>
                <td><g:TextBox ui:field="noteBox"/></td>
            </tr>
            <tr>
                <td>Admin: </td>
                <td><g:CheckBox ui:field="adminCheckBox"/></td>
            </tr>
            <tr>
                <td>Whitelisted: </td>
                <td><g:CheckBox ui:field="whitelistedCheckBox"/></td>
            </tr>
            <tr id="responsiblePersonRow">
                <td>Responsible Person: </td>
                <td><g:TextBox ui:field="responsiblePersonBox"/></td>
            </tr>
            <tr>
                <td>Active Krb account: </td>
                <td><g:CheckBox ui:field="activeAccountCheckBox"/></td>
            </tr>
        </table>
        <p class="error"><g:HTML ui:field="feedbackLabel"/></p>
        <p style="text-align: right;">
            <g:Button ui:field="cancelButton">Cancel</g:Button>
            <g:Button ui:field="submitButton">Save</g:Button>
        </p>
    </g:HTMLPanel>
</ui:UiBinder>